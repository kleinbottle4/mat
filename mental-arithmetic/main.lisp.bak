(defvar *MAX* 1000)
(defvar *OPS* '((function +) (function *) (function -) (function /)))

(setf *random-state* (make-random-state t))

(loop
  (let
	((x (random *MAX*)) (y (random *MAX*))
	(fn (nth (random (list-length *OPS*)) *OPS*)))

	(with-standard-io-syntax
	(format t "~d ~a ~d~%" x y fn)
	)
	(read)))

